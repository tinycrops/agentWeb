# API Keys - replace with your actual keys
OPENAI_API_KEY=your-openai-api-key
DISCORD_TOKEN=your-discord-token
GEMINI_API_KEY=your-gemini-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
HF_TOKEN=your-huggingface-token
GITHUB_TOKEN=your-github-token

# Server configuration
PORT=4000
NODE_ENV=development
USE_MOCKS=true

# Frontend URL for CORS
FRONTEND_URL=http://localhost:3000

# MongoDB configuration
MONGODB_URL=mongodb://localhost:27017
MONGODB_DB=agentWeb

# Redis configuration
REDIS_URL=redis://localhost:6379

# Log level
LOG_LEVEL=info

# Secret for JWT tokens (use a strong random string in production)
JWT_SECRET=your-secret-key-here

# GitHub webhook secret (for validating webhooks)
GITHUB_WEBHOOK_SECRET=your-github-webhook-secret

# CI webhook secret (for validating CI webhooks)
CI_WEBHOOK_SECRET=your-ci-webhook-secret

# Feature flags
ENABLE_FORECAST_AGENT=false
ENABLE_SCHEMA_VALIDATION=false 